{
  "name": "eace/transient",
  "description": "rex_config with expiration date for REDAXO 5",
  "license": "MIT",
  "scripts": {
    "lint": "phplint ./ --exclude=vendor --no-cache lib",
    "phpcs": "phpcs --extensions=php --standard=phpcs.xml lib",
    "phpcbf": "phpcbf --extensions=php --standard=phpcs.xml lib",
    "test": "phpunit tests/ --do-not-cache-result --colors=always -v --debug"
  },
  "autoload-dev": {
    "classmap": [
      "lib/rex_transient.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "overtrue/phplint": "^4.1"
  }
}
